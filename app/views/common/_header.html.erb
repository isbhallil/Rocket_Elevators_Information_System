 <div id="header" class="navbar-toggleable-md sticky header-lg clearfix">
<header id="topNav">
    <div class="container">

        <!-- Logo -->
        <a class="logo float-left scrollTo" href="<%= root_path()%>">
            <%= image_tag 'rocket-elevators-logo.png' %>
        </a>

        <div class="navbar-collapse collapse float-right nav-main-collapse submenu-dark">
            <nav class="nav-main">
                <ul id="topMain" class="nav nav-pills nav-main nav-onepage">
                    
                    <% @navs.each do |nav| %>
                      <%= render partial: 'common/nav-with-id', 
                                 locals: { nav: nav  } %>
                    <% end %>
                    

                </ul>
            </nav>
        </div>

    </div>
</header>
</div>
